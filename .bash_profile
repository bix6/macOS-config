# Auto generated with python install?
# Setting PATH for Python 3.7
# The original version is saved in .bash_profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/3.7/bin:${PATH}"
export PATH

# turn colors on, set colors
export CLICOLOR=1
export LSCOLORS=ExFxBxDxCxegedabagacad
# colorize bash prompt as username@hostname:cwd$
export PS1="\[\033[36m\]\u\[\033[m\]@\[\033[32m\]\h:\[\033[33;1m\]\w\[\033[m\]\$ "

# Aliases
# -F symbols after certain types e.g. / after dir
alias ls='ls -F'
# ls typo
alias sl='ls'
# -l long format, -h unit suffixes, -A show all except . and ..
alias ll='ls -lhA'
alias cd..='cd ..'
alias ..='cd ..'
alias cdd='cd ~/Desktop'
alias nbp='nano .bash_profile'
alias p3='python3'
alias va='source venv/bin/activate'
alias dis='cd ~/Desktop/a-dispatch-report'
alias p3m='python3 main.py'
alias clp='cd ~/Desktop/a-clipy'
alias gaa='git add -A'
alias gcm='git commit -m'
alias gpo='git push origin'
alias gs='git status'
alias gp='git pull'
